<div class= 'page-header'>
  <p><h1>Projects <%= link_to 'New Project', new_project_path(@project), class:"btn btn-info pull-right" %></h1></p>
</div>


<table class= 'table'>
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
    <tr>
      <td><%=link_to project.name, project_path(project) %></td>
    </tr>
    <%end%>
  </tbody>
</table>
